        "@types/multer": "^1.3.7",
        "@types/node": "^7.0.70",
        "body-parser": "^1.18.3",
        "chalk": "^1.1.3",
        "express": "^4.16.3",
        "mongodb": "^3.1.13",
        "multer": "^1.3.1",
        "reflect-metadata": "^0.1.12",
        "routing-controllers": "^0.7.7",
